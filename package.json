{
  "type": "module",
  "scripts": {
    "dev:api": "tsx watch --env-file .env index.ts",
    "dev:db": "docker compose up db",
    "format": "prettier . --write",
    "studio": "drizzle-kit studio",
    "build": "tsup index.ts --format esm --dts --clean",
    "start": "cross-env NODE_NO_WARNINGS=1 pm2-runtime start ecosystem.config.cjs --env production",
    "start:dev": "cross-env NODE_NO_WARNINGS=1 pm2-runtime start ecosystem.config.cjs",
    "monitor": "pm2 monit",
    "logs": "pm2 logs",
    "pm2:delete": "pm2 delete all",
    "test:api": "jest --runInBand --detectOpenHandles --forceExit test/ocean_dcf_backend.test.ts",
    "test:ui": "jest  --runInBand --detectOpenHandles --forceExit test/ocean_dcf_frontend.test.ts",
    "coverage": "nyc npm run test"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "express": "^4.21.1",
    "express-status-monitor": "^1.3.4",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pm2": "^5.3.1",
    "tsx": "^4.19.2",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^5.0.1",
    "@types/chai-http": "^3.0.5",
    "@types/express": "^5.0.0",
    "@types/express-status-monitor": "^1.3.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mocha": "^10.0.10",
    "@types/pg": "^8.11.10",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "chai": "^5.1.2",
    "chai-http": "^5.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.16.1",
    "drizzle-kit": "^0.26.2",
    "jest": "^29.7.0",
    "mocha": "^11.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "puppeteer": "^23.10.0",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
